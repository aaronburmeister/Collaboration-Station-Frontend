<div class="artist-list">
    <h2>Artists</h2>
    {{#each @artists as |artist|}}
        <div class="artist-card">
            <h3>{{artist.username}}</h3>
            {{#each artist.images as |image|}}
                <ArtistCards::Image  @image={{image}}/>
            {{/each}}
        </div>
    {{/each}}
</div>

{{yield}}